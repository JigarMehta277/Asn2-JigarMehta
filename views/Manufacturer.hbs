<div>
  <table>
        <thead>
          <tr>
            {{#each (lookup Manufacturer 0)}}
              <th>{{@key}}</th>
            {{/each}}
          </tr>
        </thead>
        <tbody>
          {{#each Manufacturer}}
            <tr>
              {{#each this}}
                <td>{{this}}</td>
              {{/each}}
            </tr>
          {{/each}}
        </tbody>
      </table>
</div>